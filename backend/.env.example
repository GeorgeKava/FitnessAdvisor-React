# Azure AI Search Configuration
AZURE_SEARCH_ENDPOINT=https://your-search-service.search.windows.net
AZURE_SEARCH_ADMIN_KEY=your-admin-key-here
AZURE_SEARCH_INDEX_NAME=fitness-index

# Azure OpenAI Configuration (existing)
AZURE_OPENAI_ENDPOINT=https://your-openai-service.openai.azure.com
AZURE_OPENAI_API_KEY=your-openai-api-key-here
AZURE_OPENAI_MODEL=gpt-4o
AZURE_OPENAI_API_VERSION=2024-06-01

# MCP Server Configuration (if needed)
MCP_SERVER_ENDPOINT=http://localhost:8000

# Application Settings
FLASK_ENV=development
FLASK_DEBUG=True

# Optional: Vector Search Configuration
AZURE_SEARCH_VECTOR_DIMENSIONS=1536
AZURE_SEARCH_SEMANTIC_CONFIG=fitness-semantic-config
